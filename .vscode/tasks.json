{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: gcc-14 build active file",
      "command": "/usr/bin/gcc-14",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "-DDEBUG",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}.out",
        "-std=c90"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}